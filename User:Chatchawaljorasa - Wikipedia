.mw-references-columns li { break-inside: avoid-column; }

ol.references { counter-reset: mw-ref-extends-parent 0 mw-references 0 list-item 0; }

ol.references > li { counter-increment: mw-ref-extends-parent 1 mw-references 1; counter-reset: mw-ref-extends-child 0; }

ol.references .mw-extended-references { list-style: none; margin-left: 2em; padding: 0px; }

.mw-extended-references > li::before { counter-increment: mw-ref-extends-child 1; content: counter(mw-ref-extends-parent) "." counter(mw-ref-extends-child) ". "; }

sup.reference { unicode-bidi: isolate; white-space: nowrap; font-weight: normal; font-style: normal; }

ol.references li:target, sup.reference:target { background-color: var(--background-color-progressive-subtle,#f1f4fd); }

